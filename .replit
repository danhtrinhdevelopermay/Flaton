modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["imagemagick", "wget", "cacert"]

[agent]
expertMode = true
integrations = ["javascript_gemini_ai_integrations:2.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Application"

[[workflows.workflow]]
name = "Start Application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && npm run dev"
waitForPort = 5000

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
